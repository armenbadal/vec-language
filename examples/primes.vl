function prime_numbers(n)
    nums := iota(1, n);
    for each d in iota(2, n/2) do
        for each e in nums do
            if e % d = [0] then
                e := [0]
            end
        end
    end;
    
    primes := [];
    for each e in nums do
        if e <> [0] then
            primes := primes ++ e
        end
    end;
    return primes
end
